<?xml version="1.0" encoding="UTF-8"?>
<urlTokens xmlns="http://www.edmunds.com/etm/1.0">

    <token name="color" type="fixed">
        <value>red</value>
        <value>green</value>
        <value>blue</value>
    </token>

    <token name="model" type="regex">
        <value>[^/]*</value>
    </token>

    <token name="year" type="regex">
        <value>(19|20)\d{2}</value>
    </token>

</urlTokens>
